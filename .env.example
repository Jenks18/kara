# Environment Variables Template
# Copy this to .env.local for local development
# Add these to Vercel dashboard for production

# ==========================================
# REQUIRED - Supabase Configuration
# ==========================================
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# ==========================================
# REQUIRED - AI/OCR Services
# ==========================================
# Get from: https://ai.google.dev/
GEMINI_API_KEY=your-gemini-api-key-here

# ==========================================
# OPTIONAL - Authentication (Clerk)
# ==========================================
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_...
CLERK_SECRET_KEY=sk_test_...

# ==========================================
# OPTIONAL - Receipt Processing Configuration
# ==========================================
# Enable AI enhancement (default: true)
ENABLE_AI_ENHANCEMENT=true

# Confidence threshold for AI processing (default: 70)
# Higher = less AI usage, lower costs, but may need more manual review
AI_CONFIDENCE_THRESHOLD=70

# Maximum retries for KRA scraping (default: 3)
RECEIPT_PROCESSING_MAX_RETRIES=3

# Processing timeout in milliseconds (default: 30000)
RECEIPT_PROCESSING_TIMEOUT_MS=30000

# ==========================================
# OPTIONAL - Development/Debugging
# ==========================================
# Enable debug logging (default: false)
DEBUG_RECEIPT_PROCESSING=false

# Log processing performance (default: false)
LOG_PERFORMANCE_METRICS=true
